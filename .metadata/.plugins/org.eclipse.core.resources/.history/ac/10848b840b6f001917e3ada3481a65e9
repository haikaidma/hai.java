package string;

public class Stringcomparison {

	public static void main(String[] args) {
		//ex by equals() method
		String s1="duy tan";
		String s2="duy tan";
		String s3= new String("duy tan");
		String s4= "duy tan1";;
		System.out.println(s1.equals(s2));
		System.out.println(s1.equals(s3));
		
	}

}
